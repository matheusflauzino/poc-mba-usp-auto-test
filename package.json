{
  "name": "credit-simulation",
  "version": "1.0.0",
  "description": "Simulador de Financiamento",
  "scripts": {
    "watch": "ts-node-dev -r tsconfig-paths/register --transpile-only src/index.ts",
    "setup": "node tasks/setup.mjs",
    "build": "npm install && node tasks/build.mjs",
    "lint": "tsc --noEmit && eslint src/**/*.ts --cache --max-warnings=0",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "vitest run --silent --allowOnly --dir ./tests/unit",
    "test:log": "vitest run --allowOnly --dir ./tests/unit",
    "test:watch": "vitest watch --allowOnly --dir ./tests/unit",
    "test:coverage": "vitest run --coverage --dir ./tests/unit",
    "test:verbose": "vitest run --reporter verbose --allowOnly",
    "test:api": "node tasks/check-version.mjs && vitest run --dir ./tests/api",
    "generate-changelog": "./tasks/changelog.sh"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.650.0",
    "@aws-sdk/client-sqs": "^3.650.0",
    "@aws-sdk/client-ssm": "^3.650.0",
    "awilix": "^11.0.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "module-alias": "^2.2.3",
    "node-forge": "^1.3.1",
    "pino": "^9.6.0",
    "undici": "^5.23.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/node": "^22.9.0",
    "@types/node-forge": "^1.3.11",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "openapi-types": "^12.1.3",
    "pino-pretty": "^13.0.0",
    "prettier": "^2.7.1",
    "resolve-tspaths": "^0.7.0",
    "sequelize-cli": "^6.6.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2",
    "vitest": "^2.0.5 ",
    "yaml": "^2.6.0"
  }
}
